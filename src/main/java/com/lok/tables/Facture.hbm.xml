<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lok.tables.Facture" table="facture" schema="bdd">
        <id name="idFacture">
            <column name="idFacture" sql-type="int(11)"/>
        </id>
        <property name="idCommande">
            <column name="IdCommande" sql-type="int(11)"/>
        </property>
        <property name="dateFacture">
            <column name="dateFacture" sql-type="date"/>
        </property>
        <property name="prixTotal">
            <column name="prixTotal" sql-type="int(20)"/>
        </property>
    </class>
</hibernate-mapping>