<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lok.tables.Enchere" table="enchere" schema="bdd">
        <id name="idEnchere">
            <column name="idEnchere" sql-type="int(11)"/>
        </id>
        <property name="idVent">
            <column name="id_vent" sql-type="int(11)"/>
        </property>
        <property name="dateDebut">
            <column name="dateDebut" sql-type="date"/>
        </property>
        <property name="dateFin">
            <column name="dateFin" sql-type="date"/>
        </property>
        <property name="dateVent">
            <column name="dateVent" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>