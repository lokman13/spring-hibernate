<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lok.tables.Livraison" table="livraison" schema="bdd">
        <id name="idLivraison">
            <column name="idLivraison" sql-type="int(11)"/>
        </id>
        <property name="dateLivraison">
            <column name="dateLivraison" sql-type="date"/>
        </property>
        <property name="fraisLivraison">
            <column name="fraisLivraison" sql-type="int(20)"/>
        </property>
    </class>
</hibernate-mapping>