<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lok.tables.Paiementclassique" table="paiementclassique" schema="bdd">
        <id name="idPaiementClassique">
            <column name="id_paiement_classique" sql-type="int(11)"/>
        </id>
        <property name="adressLivraison">
            <column name="adressLivraison" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="dateLivraison">
            <column name="dateLivraison" sql-type="date"/>
        </property>
        <property name="idPaiement">
            <column name="idPaiement" sql-type="int(11)"/>
        </property>
        <property name="datePaiement">
            <column name="datePaiement" sql-type="date"/>
        </property>
    </class>
</hibernate-mapping>